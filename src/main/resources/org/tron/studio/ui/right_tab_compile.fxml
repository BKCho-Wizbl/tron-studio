<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXCheckBox?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXToggleButton?>
<VBox spacing="10" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.tron.studio.ui.RightTabCompileController">
    <padding>
        <Insets top="10" right="10" bottom="10" left="10"/>
    </padding>
    <VBox spacing="10">
        <JFXButton styleClass="custom-jfx-button-raised" onAction="#onClickCompile" text="Compile"/>
        <JFXToggleButton fx:id="autoCompileToggleButton" onAction="#onClickAutoCompile" text="Auto Compile"/>
        <HBox spacing="10">
            <children>
                <JFXComboBox fx:id="contractComboBox" HBox.hgrow="ALWAYS"/>
                <JFXButton styleClass="custom-jfx-button-raised" onAction="#onClickDetail" text="Detail"/>
            </children>
        </HBox>
    </VBox>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
</VBox>
